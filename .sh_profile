# Environment variables
export EDITOR=nvim
export QT_QPA_PLATFORMTHEME=qt6ct
export WINEARCH=win64
export WINEPREFIX=~/wine/main
export ANDROID_HOME="$HOME/libs/Android"
export FLUTTER_SDK="$HOME/libs/Flutter"
export DART_SDK="$HOME/libs/Flutter/bin/cache/dart-sdk/"
export ANDROID_SDK_HOME="$HOME/libs/Android"
export ANDROID_AVD_HOME="$HOME/libs/Android/avd"
export JAVA_HOME="/usr/lib/jvm/default"
export PATH="$HOME/.local/bin:$FLUTTER_SDK/bin:$DART_SDK/bin:$PATH"
export HISTSIZE=1000
export SAVEHIST=1000
export HISTFILE=~/.sh_history
export XDG_CONFIG_HOME=~/.config
export MANPAGER="nvim +Man!"
